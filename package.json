{
  "name": "url2go",
  "version": "1.0.0",
  "description": "URL2GO Desktop Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --mac",
    "build": "electron-builder"
  },
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "electron": "^29.1.1",
    "electron-builder": "^24.13.3",
    "@electron/rebuild": "^3.6.0"
  },
"build": {
  "mac": {
    "target": "dmg",
    "icon": "icon/icon.icns",
    "dmg": {
      "background": "path/to/background.png",
      "backgroundColor": "#ffffff",
      "iconSize": 80,
      "iconTextSize": 12,
      "title": "${productName} ${version}",
      "contents": [
        {
          "x": 100,
          "y": 100,
          "type": "link",
          "path": "/Applications",
          "name": "Applications"
        }
      ],
      "window": {
        "x": 400,
        "y": 100,
        "width": 600,
        "height": 400
      },
      "internetEnabled": false,
      "sign": false
    }
  }
}



}
